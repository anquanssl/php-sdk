{
    "name": "quantumca/sdk",
    "description": "The php sdk for QuantumCA.",
    "type": "library",
    "authors": [
        {
            "name": "tech",
            "email": "phpsdk@quantumca.com.cn"
        }
    ],
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "require": {
        "php": "^5.6 || ^7.0 || ^8.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "QuantumCA\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QuantumCA\\Sdk\\Test\\": "tests/"
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^9.5"
    },
    "suggest": {
        "illuminate/http": "Using for better interactives"
    }
}
